{
  "name": "api",
  "version": "0.0.1",
  "main": "bin/www.js",
  "author": "abejaoui",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run build && node dist/bin/www.js",
    "build": "npm run clean && tsc --project tsconfig.prod.json",
    "clean": "rimraf dist",
    "dev": "npm run eslint && npm run watch-ts",
    "eslint": "eslint --quiet --cache ./src",
    "run:watch": "nodemon dist/bin/www.js",
    "watch-ts": "tsc-watch --onSuccess \"npm run run:watch\"",
    "pre-commit": "npm run eslint-fix",
    "eslint-fix": "eslint --fix --quiet ./src",
    "test": "jest",
    "coverage": "jest --coverage",
    "pre-push": "npm run test"
  },
  "devDependencies": {
    "@jest/globals": "29.3.1",
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.15",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "8.30.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "kill-port": "2.0.1",
    "nodemon": "2.0.20",
    "prettier": "2.8.1",
    "proxyquire": "2.1.3",
    "sinon": "15.0.1",
    "ts-jest": "29.0.3"
  },
  "dependencies": {
    "body-parser": "1.20.0",
    "chalk": "4.1.2",
    "cors": "2.8.5",
    "envalid": "7.3.1",
    "express": "4.18.1",
    "express-validator": "6.14.2",
    "http-status": "1.5.3",
    "http-terminator": "3.2.0",
    "mongoose": "6.6.0",
    "postmark": "3.0.15",
    "rimraf": "3.0.2",
    "typescript": "4.9.4",
    "ts-node": "10.9.1",
    "dotenv": "16.0.3",
    "tsc-watch": "6.0.0"
  }
}
